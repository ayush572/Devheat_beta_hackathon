  <!-- Enter code here -->
  <p>
  <h2>Available centeres</h2>
  </p>
  <% for(var i=0;i<avcen.length;i++) {%>

    <div class="list-group">
      <button type="button" class="list-group-item list-group-item-action">
        <%= avcen[i].cities[0] %>
      </button>
      <button type="button" class="list-group-item list-group-item-action">
        <%= avcen[i].cities[1] %>
      </button>
      <button type="button" class="list-group-item list-group-item-action">
        <%= avcen[i].cities[2] %>
      </button>
      <button type="button" class="list-group-item list-group-item-action">
        <%= avcen[i].cities[3] %>
      </button>
      <button type="button" class="list-group-item list-group-item-action">
        <%= avcen[i].cities[4] %>
      </button>
    </div>

    <% } %>
